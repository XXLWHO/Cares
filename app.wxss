/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.f {
  display: flex;
}

.f-j {
  display: flex;
  justify-content: center;
}

.f-a {
  display: flex;
  align-items: center;
}

.f-aj {
  display: flex;
  justify-content: center;
  align-items: center;
}

.f-j-s {
  display: flex;
  justify-content: space-between;
}

.sheme-color {
  color: #63CAFD;
}

/* 去掉滚动条 */
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent ;
  display: none;
}
/* 主题颜色 */
.blue{
  background-color: #4ABCFC;
  color: white;
}